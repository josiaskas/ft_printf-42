{
    "tasks": [
                {
            "label": "creer la librairie",
            "command": "/usr/bin/make",
            "args": [
                "re",
                "-C",
                "${fileDirname}",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Tâche générée par le make file de la librairie"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc générer le fichier actif",
            "dependsOn": [
                "creer une librairie",
            ],
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-L${fileDirname}",
                "-lftprintf"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Tâche générée par le débogueur."
        }
    ],
    "version": "2.0.0"
}
